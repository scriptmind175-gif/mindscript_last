{
  "version": 2,
  "alias": ["mindscript.online", "www.mindscript.online"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "REACT_APP_API_BASE_URL": "https://mindscript.online",
    "REACT_APP_RAZORPAY_KEY_ID": "rzp_live_R6kzNc1g06owLV"
  },
  "build": {
    "env": {
      "CI": "false",
      "REACT_APP_API_BASE_URL": "https://mindscript.online",
      "REACT_APP_RAZORPAY_KEY_ID": "rzp_live_R6kzNc1g06owLV"
    }
  },
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ]
}
