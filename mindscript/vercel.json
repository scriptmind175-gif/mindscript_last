{
  "version": 2,
  "alias": ["mindscript.online", "www.mindscript.online"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://mindscript.online",
    "REACT_APP_RAZORPAY_KEY_ID": "rzp_live_R6kzNc1g06owLV"
  },
  "build": {
    "env": {
      "CI": "false",
      "REACT_APP_API_BASE_URL": "https://mindscript.online",
      "REACT_APP_RAZORPAY_KEY_ID": "rzp_live_R6kzNc1g06owLV"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://mindscript-backend-fresh.vercel.app/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
